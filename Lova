    if event.fwd_from:
        return
       
 
    await event.edit("ты лох")
    await asyncio.sleep(0.3)
    await event.edit("ты гей")
    await asyncio.sleep(0.3)
    await event.edit("ты чмо")
    await asyncio.sleep(0.3)
    await event.edit("ты даун")
    await asyncio.sleep(0.3)
    await event.edit("ты ебалн")
    await asyncio.sleep(0.3)
    await event.edit("ты хуесос")
